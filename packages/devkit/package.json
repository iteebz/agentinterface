{
  "name": "@agentinterface/devkit",
  "version": "0.1.0",
  "description": "Development tools and generators for agentinterface",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "aip-gen": "./dist/generate.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,json,md}'",
    "format:check": "prettier --check '**/*.{ts,tsx,js,jsx,json,md}'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@types/node": "^24.0.14",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "agentinterface",
    "devkit",
    "generators",
    "aip"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iteebz/agentinterface",
    "directory": "packages/devkit"
  },
  "license": "MIT"
}