{
  "name": "@agentinterface/studio",
  "version": "0.1.0",
  "description": "Interactive studio for testing and developing agentinterface components",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,json,md,html,css}'",
    "format:check": "prettier --check '**/*.{ts,tsx,js,jsx,json,md,html,css}'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "agentinterface": "workspace:*",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@types/node": "^24.0.15",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.7",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "agentinterface",
    "studio",
    "testing",
    "components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iteebz/agentinterface",
    "directory": "packages/studio"
  },
  "license": "MIT"
}
