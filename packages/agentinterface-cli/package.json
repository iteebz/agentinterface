{
  "name": "agentinterface-cli",
  "version": "0.1.0",
  "description": "Build tools for AgentInterface SSR registry generation",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agentinterface-cli": "dist/registry/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,json,md}'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "agentinterface": "workspace:*",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.0.0",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "tsup": "^8.0.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.19.0",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
