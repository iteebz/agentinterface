{
  "name": "agentinterface",
  "version": "1.0.0",
  "description": "Agent GUIs (React SDK)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agentinterface": "./cli/index.js"
  },
  "files": [
    "src",
    "cli",
    "scripts",
    "dist"
  ],
  "scripts": {
    "discover": "node scripts/discover.mjs",
    "discover:quiet": "node scripts/discover.mjs --quiet",
    "dev": "echo 'Import components directly'",
    "test": "npm run test:unit",
    "visual": "node scripts/visual-inspect.js",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write --log-level=error 'src/**/*.{ts,tsx}' 'tests/**/*.{ts,tsx}'",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "build": "tsc",
    "prepare": "npm run build",
    "test:unit": "vitest run"
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "1.1.12",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.525.0",
    "react-markdown": "10.1.0",
    "react-syntax-highlighter": "15.5.0",
    "remark-gfm": "4.0.1",
    "tailwind-merge": "2.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "@testing-library/react": "^16.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@vitejs/plugin-react": "^5.0.3",
    "jsdom": "^24.1.3",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^3.4.0"
  },
  "keywords": [
    "react",
    "ai",
    "agents",
    "interface",
    "ui",
    "components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iteebz/agentinterface"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "packageManager": "npm@10.9.0"
}
