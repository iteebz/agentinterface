{
  "name": "agentinterface-workspace",
  "private": true,
  "description": "Monorepo for agentinterface packages",
  "scripts": {
    "build": "pnpm --filter '*' run build",
    "dev": "pnpm --filter agentinterface run dev",
    "test": "pnpm --filter '*' run test",
    "format": "pnpm --filter '*' run format",
    "lint": "pnpm --filter '*' run lint",
    "clean": "rm -rf packages/*/dist packages/*/node_modules"
  },
  "devDependencies": {
    "pnpm": "^10.12.1"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac",
  "license": "MIT"
}